@page "{handler?}"
@model WebArchivProject.Areas.Identity.Pages.IndexModel
@using static WebArchivProject.Helper.StringConstant
@{
    ViewData["Title"] = "Авторизація";
}

@if (Model.IdentityState == LOGIN)@await Html.PartialAsync("_Partial_Login")
@if (Model.IdentityState == REGISTER)@await Html.PartialAsync("_Partial_Register")